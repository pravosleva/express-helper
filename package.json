{
  "name": "express-helper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "kill-5000": "fuser -k -n tcp 5000",
    "build:front": "yarn --cwd routers/auth/pages/signin install && yarn --cwd routers/auth/pages/signin build",
    "start": "yarn build:front && PORT=5000 NODE_PATH=. node ./bin/www",
    "dev": "yarn build:front && SUCCESS_ANYWAY=1 DEBUG=express-helper:* npm start",
    "start:pm2": "pm2 start npm --name \"express-helper-5000\" -- start",
    "test": "jest",
    "deploy:prod:restart-all": "deploy-app -d prod:restart-all",
    "deploy:prod:restart-helper": "deploy-app -d prod:restart-helper",
    "lint": "eslint --debug 'routers/**/*.{js,jsx,ts,tsx}' 'utils/**/*.{js,jsx,ts,tsx}' 'bin/**/*' app.js",
    "lint:fix": "eslint 'routers/**/*.{js,jsx,ts,tsx}' 'utils/**/*.{js,jsx,ts,tsx}' 'bin/**/*' app.js --fix"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": [
      "node_modules/.bin/eslint --max-warnings=0 --no-ignore",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "deploy-app": "^1.0.3",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.5.1",
    "md5-hash": "^1.0.1",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "node-imei": "^1.0.8",
    "swagger-ui-express": "^4.1.4",
    "url-metadata": "^2.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  }
}
