{
  "name": "express-helper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:chat": "bash run-dev.sh chat",
    "dev:signin": "bash run-dev.sh signin",
    "dev": "bash run-dev.sh",
    "deploy:prod:copy-chat-build": "bash src/set-front.chat-env.sh prod && yarn --cwd src/frontend.chat build && deploy-app -d prod:copy-chat-build",
    "deploy:prod:copy-helper": "deploy-app -d prod:copy-helper",
    "deploy:prod:restart-helper": "yarn build:fresh-prod && deploy-app -d prod:restart-helper",
    "deploy:smartprice:restart-helper": "yarn build:fresh-prod && deploy-app -d smartprice:restart-helper",
    "deploy:prod:restart-all": "deploy-app -d prod:restart-all",
    "kill-5000": "fuser -k -n tcp 5000",
    "cleanup:server-dist": "rm -rf ./server-dist/* !./server-dist/.gitkeep",
    "transpile-server:watch": "tsc -p ./src/server -w",
    "quaint-html-copy": "bash quaint-files-copy.sh src/server/ server-dist/ html",
    "quaint-css-copy": "bash quaint-files-copy.sh src/server/ server-dist/ css",
    "quaint-yaml-copy": "bash quaint-files-copy.sh src/server/ server-dist/ yaml",
    "quaint-express-env-copy:dev": "cp .env.dev server-dist/.env",
    "quaint-express-env-copy:prod": "cp .env.prod server-dist/.env",
    "quaint-copy:dev": "yarn quaint-yaml-copy && yarn quaint-css-copy && yarn quaint-html-copy && yarn quaint-express-env-copy:dev",
    "quaint-copy:prod": "yarn quaint-yaml-copy && yarn quaint-css-copy && yarn quaint-html-copy && yarn quaint-express-env-copy:prod",
    "transpile-server:dev": "tsc -p ./src/server && yarn quaint-copy:dev",
    "transpile-server:prod": "tsc -p ./src/server && yarn quaint-copy:prod",
    "build:front.chat:dev": "yarn --cwd src/frontend.chat install && bash src/set-front.chat-env.sh dev && yarn --cwd src/frontend.chat build",
    "build:front.chat:prod": "yarn --cwd src/frontend.chat install && bash src/set-front.chat-env.sh prod && yarn --cwd src/frontend.chat build",
    "build:front.singin:dev": "yarn --cwd src/frontend.signin install && bash src/set-front.signin-env.sh dev && yarn --cwd src/frontend.signin build",
    "build:front.signin:prod": "yarn --cwd src/frontend.signin install && bash src/set-front.signin-env.sh prod && yarn --cwd src/frontend.signin build",
    "build:fresh-prod": "yarn cleanup:server-dist && yarn transpile-server:prod && yarn build:front.signin:prod && yarn build:front.chat:prod",
    "build:fresh-dev:chat-only": "yarn cleanup:server-dist && yarn transpile-server:dev && yarn build:front.chat:dev",
    "build:fresh-dev": "yarn cleanup:server-dist && yarn transpile-server:dev && yarn build:front.singin:dev && yarn build:front.chat:dev",
    "start": "PORT=5000 NODE_PATH=. node ./bin/www",
    "start:pm2": "pm2 start npm --name \"express-helper-5000\" -- start",
    "test": "jest",
    "lint": "eslint --debug 'src/server/**/*.{js,jsx,ts,tsx}' 'bin/**/*'",
    "lint:fix": "eslint 'src/server/**/*.{js,jsx,ts,tsx}' '!src/server/routers/chat/@socket.io/**/*' 'bin/**/*' --fix"
  },
  "_moduleAliases": {
    "~": "./server-dist",
    "@getRandomInteger": "./server-dist/utils/getRandomInteger.js"
  },
  "lint-staged": {
    "src/server/**/*.{js, jsx, ts, tsx} !src/server/routers/chat/@socket.io/**/*": [
      "node_modules/.bin/eslint --max-warnings=0 --no-ignore",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@socket.io/admin-ui": "^0.2.0",
    "animate.css": "^4.1.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "build-url": "^6.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cyberjon-express-formidable": "^1.0.1",
    "debug": "~2.6.9",
    "deploy-app": "^1.0.3",
    "device-detector-js": "^2.2.10",
    "dotenv": "^8.2.0",
    "es6-promisify": "^6.1.1",
    "express": "~4.16.0",
    "express-formidable": "^1.2.0",
    "express-request-id": "^1.4.1",
    "js-base64": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "md5-hash": "^1.0.1",
    "merge-deep": "^3.0.3",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "node-imei": "^1.0.8",
    "qrcode": "^1.4.4",
    "request": "^2.88.2",
    "shelljs": "^0.8.4",
    "socket.io": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "systeminformation": "^5.8.7",
    "universal-cookie": "^4.0.4",
    "url-metadata": "^2.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.11",
    "@types/express-request-id": "^1.4.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/qrcode": "^1.4.0",
    "@types/request": "^2.48.7",
    "@types/shelljs": "^0.8.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^4.2.3"
  }
}
